<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />

  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />

  <%= f.label :genre_name %>
  <%= f.collection_select :genre_name, Genre.all, :name, :name, {}, {id: "song_genre_id"} %>
  <br />

  <%= f.label :song_notes_1 %>
  <%= f.text_field :note_contents, id: "song_notes_1", name: "song[note_contents][]" %>
  <br />

  <%= f.label :song_notes_2 %>
  <%= f.text_field :note_contents, id: "song_notes_2", name: "song[note_contents][]" %>
  <br />



  <%= f.submit %>
<% end %>


